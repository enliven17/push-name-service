{
  "name": "push-name-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "compile": "hardhat compile",
    "deploy:push-chain": "hardhat run scripts/deploy-push-name-service.js --network pushChainDonut",
    "deploy:ethereum-sepolia": "hardhat run scripts/deploy-push-name-service.js --network ethereumSepolia",
    "deploy:ethereum": "hardhat run scripts/deploy-push-name-service.js --network ethereum",
    "deploy:polygon": "hardhat run scripts/deploy-push-name-service.js --network polygon",
    "deploy:bsc": "hardhat run scripts/deploy-push-name-service.js --network bsc",
    "deploy:arbitrum": "hardhat run scripts/deploy-push-name-service.js --network arbitrum",
    "deploy:optimism": "hardhat run scripts/deploy-push-name-service.js --network optimism",
    "deploy:local": "hardhat run scripts/deploy-push-name-service.js --network localhost",
    "deploy:testnet": "npm run deploy:push-chain && npm run deploy:ethereum-sepolia",
    "deploy:all": "npm run deploy:push-chain && npm run deploy:ethereum && npm run deploy:polygon && npm run deploy:bsc && npm run deploy:arbitrum && npm run deploy:optimism",
    "test": "hardhat test",
    "test:push-chain": "hardhat run scripts/test-push-chain.js --network pushChainDonut",
    "test:ethereum-sepolia": "hardhat run scripts/test-push-chain.js --network ethereumSepolia"
  },
  "dependencies": {
    "@creit.tech/stellar-wallets-kit": "^1.7.6",
    "@pushprotocol/restapi": "^1.7.32",
    "@pushprotocol/socket": "^0.5.3",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@stellar/wallet-sdk": "^0.11.2",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.90.2",
    "@types/styled-components": "^5.1.34",
    "@walletconnect/ethereum-provider": "^2.21.8",
    "@walletconnect/modal": "^2.7.0",
    "@walletconnect/web3-provider": "^1.8.0",

    "babel-plugin-styled-components": "^2.1.4",
    "clsx": "^2.1.1",
    "ethers": "^6.9.0",
    "next": "15.4.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "simplex-noise": "^4.0.3",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1",
    "viem": "^2.37.8",
    "wagmi": "^2.17.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@moved/hardhat-plugin": "^0.2.1",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@openzeppelin/contracts": "^4.9.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^16.3.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "hardhat": "^2.19.0",
    "typescript": "^5"
  }
}
